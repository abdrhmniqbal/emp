{
  "expo": {
    "newArchEnabled": true,
    "scheme": "euphony-music",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static"
    },
    "name": "Euphony Music",
    "slug": "emp",
    "extra": {
      "eas": {
        "projectId": "5db0649c-cba6-4a3a-b862-40d8e83f4e2c"
      }
    },
    "experiments": {
      "reactCompiler": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "id.euphony.music",
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "package": "id.euphony.music",
      "permissions": [
        "WAKE_LOCK",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_AUDIO_PLAYBACK"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-media-library",
      "@zoontek/react-native-navigation-bar",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0088F6",
          "image": "./src/assets/splash-icon.png",
          "dark": {
            "image": "./src/assets/splash-icon.png",
            "backgroundColor": "#09090b"
          },
          "resizeMode": "contain"
        }
      ],
      [
        "react-native-file-viewer-turbo",
        {
          "mimeTypes": ["*/*"]
        }
      ]
    ]
  }
}
